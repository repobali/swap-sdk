    "build": "npm run clean && npm run build-without-clean"
  },
  "devDependencies": {
    "@types/node": "^18.8.5",
    "prompt-sync": "^4.2.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "yaml": "^2.1.3"
  },
  "dependencies": {
    "@manahippo/move-to-ts": "^0.4.2",
    "@mysten/sui.js": "^0.12.0",
    "aptos": "^1.3.16",
    "axios": "^1.1.2"
  }
}
npm install @vividnetwork/swap-sdk
